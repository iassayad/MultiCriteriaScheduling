#!/bin/sh
# If necessary, set your install directory instead on next line
export INSTALL_DIR="."
# if required /usr/local/lib/tcl8.3 or higher
export TCL_LIBRARY=
# if required /usr/local/lib/tk8.3 or higher
export TK_LIBRARY=
if [ ! $?LD_LIBRARY_PATH ]
then export LD_LIBRARY_PATH=""
fi
export LD_LIBRARY_PATH=${LD_LIBRARY_PATH}:${TCL_LIBRARY}:${TK_LIBRARY}
${INSTALL_DIR}/bin/syndex-tui -libs ${INSTALL_DIR}/libs $* &
